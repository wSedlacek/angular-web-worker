{
  "version": 1,
  "projects": {
    "angular-web-worker": {
      "root": "libs/angular-web-worker",
      "sourceRoot": "libs/angular-web-worker/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/angular-web-worker/tsconfig.lib.json",
            "project": "libs/angular-web-worker/src/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/angular-web-worker/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/angular-web-worker/tsconfig.lib.json",
              "libs/angular-web-worker/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/angular-web-worker/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/angular-web-worker/jest.config.js",
            "tsConfig": "libs/angular-web-worker/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "libs/angular-web-worker/src/test-setup.ts"
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/workspace"
  }
}
