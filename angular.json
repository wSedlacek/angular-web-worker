{
  "version": 1,
  "projects": {
    "angular-web-worker": {
      "root": "libs/angular-web-worker",
      "sourceRoot": "libs/angular-web-worker/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/angular-web-worker/tsconfig.lib.json",
            "project": "libs/angular-web-worker/src/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/angular-web-worker/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/angular-web-worker/tsconfig.lib.json",
              "libs/angular-web-worker/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/angular-web-worker/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/angular-web-worker/jest.config.js",
            "tsConfig": "libs/angular-web-worker/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "libs/angular-web-worker/src/test-setup.ts"
          }
        }
      }
    },
    "samples-basic": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/samples/basic",
      "sourceRoot": "apps/samples/basic/src",
      "prefix": "angular-web-worker",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/samples/basic",
            "index": "apps/samples/basic/src/index.html",
            "main": "apps/samples/basic/src/main.ts",
            "polyfills": "apps/samples/basic/src/polyfills.ts",
            "tsConfig": "apps/samples/basic/tsconfig.app.json",
            "aot": true,
            "assets": ["apps/samples/basic/src/favicon.ico", "apps/samples/basic/src/assets"],
            "styles": ["apps/samples/basic/src/styles.scss"],
            "scripts": [],
            "webWorkerTsConfig": "apps/samples/basic/tsconfig.worker.json"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/samples/basic/src/environments/environment.ts",
                  "with": "apps/samples/basic/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "samples-basic:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "samples-basic:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "samples-basic:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/samples/basic/tsconfig.app.json",
              "apps/samples/basic/tsconfig.spec.json",
              "apps/samples/basic/tsconfig.worker.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/samples/basic/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/samples/basic/jest.config.js",
            "tsConfig": "apps/samples/basic/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/samples/basic/src/test-setup.ts"
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  }
}
